<?xml version="1.0"?>
<block>
  <name>detector</name>
  <key>drone_filter_detector</key>
  <category>[drone_filter]</category>
  <import>import drone_filter</import>
  <make>drone_filter.detector($inputs)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Values</name>
    <key>inputs</key>
    <type>complex</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>

  
</block>
